# Example environment variables for Video Splitter Pro
# Copy this to .env and fill in your actual values

# JWT Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-here
JWT_REFRESH_SECRET=your-super-secure-refresh-secret-key-here

# AWS Configuration
AWS_REGION=us-east-1
FRONTEND_URL=https://develop.tads-video-splitter.com

# MongoDB Configuration
MONGO_URL=mongodb://your-mongodb-host:27017

# Email Configuration (for SES)
SES_SENDER_EMAIL=noreply@tads-video-splitter.com
SES_SENDER_NAME=Tad's Video Splitter

# API Gateway URL (for frontend)
VITE_REACT_APP_API_GATEWAY_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod